# Supabase Project URL (Settings > API)
NEXT_PUBLIC_SUPABASE_URL=https://YOUR_PROJECT_REF.supabase.co

# Supabase anon public key (Settings > API)
NEXT_PUBLIC_SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY

# Server-only key for admin/server actions (never expose on client)
SUPABASE_SERVICE_ROLE_KEY=YOUR_SUPABASE_SERVICE_ROLE_KEY

# Comma-separated list of admin emails used by the admin guard bootstrap
ADMIN_EMAILS=admin@example.com,ops@example.com
